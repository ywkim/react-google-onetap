      "react"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.4",
    "prettier": "^1.12.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "dependencies": {
    "prop-types": "^15.6.1",
    "recompose": "^0.27.0",
    "scriptjs": "^2.5.8"
  }
